{%- comment -%} "theme-default-pagination" this Class is needed for active page pagination button disable{%- endcomment -%}
{% if paginate.pages > 1 %}
<div class="theme-default-pagination paginatoin-area text-center">
  <ul class="pagination-box product-pagination-area">
    {% if paginate.previous %}
    <li>
      <a href="{{ paginate.previous.url }}" aria-label="Previous">
        <span aria-hidden="true"><i class="pe-7s-angle-left"></i></span>
      </a>
    </li>
    {% else %}
    <li class="disabled prev">
      <a href="#">
        <span><i class="pe-7s-angle-left"></i></span>
      </a>
    </li>
    {% endif %}
    {% for part in paginate.parts %}
    {% if part.is_link %}
    <li>
      <a href="{{ part.url }}" title="">{{ part.title }}</a>
    </li>
    {% else %}
    {% if part.title == paginate.current_page %}
    <li class="active"><a href="">{{ part.title }}</a></li>
    {% else %}
    <li><a href="#">{{ part.title }}</a></li>
    {% endif %}
    {% endif %}
    {% endfor %}
    {% if paginate.next %}
    <li class="next"><a href="{{ paginate.next.url }}" title="{{ paginate.next.title }}"><span aria-hidden="true"><i class="pe-7s-angle-right"></i></span></a></li>
    {% else %}
    <li class="disabled next">
      <a href="#">
        <span><i class="pe-7s-angle-right"></i></span>
      </a>
    </li>
    {% endif %}
  </ul>
</div>
{% endif %}

<script>
  $(".theme-default-pagination .disabled a").removeAttr("href");
  $(".theme-default-pagination li.active a").removeAttr("href");
</script>