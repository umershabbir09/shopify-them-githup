    <div class="banner banner-3 banner-5"  id="section-{{ section.id }}" data-section="SlideShow_8" >
      <div class="swiper bannerSlide2">
          <div class="swiper-wrapper">
         {%- for block in section.blocks -%}
          {%- case block.type -%}
    {%- when 'slide' -%}  
              <div class="swiper-slide">
                  <div class="banner-single bg-image-5 balo">
                      <div class="container">
                          <div class="single-inner">
                              <div class="contents">
            {%- if block.settings.slide_sub_title != '' -%}
                                  <span class="banner-pretitle-box">{{ block.settings.slide_sub_title }} </span>
            {%- endif -%}
            {%- if block.settings.slide_title != '' -%}
                                  <h1 class="banner-heading mb--30">{{ block.settings.slide_title }}</h1>
            {%- endif -%}
                                  <div class="banner-action">
                                      <p>{{ block.settings.slide_description}}</p>
                                      <span class="product-price">{{ block.settings.slide_price}}</span>
                                  </div>
                              </div>
                              <div class="banner-product-thumb">
                                <img src="{{ block.settings.slide_image | img_url: 'master' }}" alt="{{ block.settings.slide_image.alt }}">
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
          {%- endcase -%}
          {%- endfor -%}
          </div>
      </div>
      <div class="banner5 floating-elements">
          <div class="floating-item item1"><img src="{{ section.settings.gros_image_1 | img_url: 'large' }}" alt="{{ section.settings.gros_image_1.alt }}"></div>
          <div class="floating-item item2"><img src="{{ section.settings.gros_image_2 | img_url: 'small' }}" alt="{{ section.settings.gros_image_2.alt }}"></div>
          <div class="floating-item item3"><img src="{{ section.settings.gros_image_3 | img_url: 'small' }}" alt="{{ section.settings.gros_image_3.alt }}"></div>
      </div>
  </div>

  <style>
    .banner .banner-single.bg-image-5 {
  background-image: url("{{ section.settings.bg_image | img_url: 'master' }}");
}
