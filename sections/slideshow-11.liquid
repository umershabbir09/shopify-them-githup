    <div class="banner banner-1 banner-8 bg-image"  id="section-{{ section.id }}" data-section="SlideShow_10">
        <div class="container">
            <div class="banner-inner">
                <div class="row">
                    <div class="col-xl-2 col-md-4 col-sm-12 gutter-1">
                        <div class="catagory-sidebar">
                            <div class="widget-bg">
            {%- if section.settings.catagory_title != '' -%}
                                <h2 class="widget-title">{{ section.settings.catagory_title }}  <i class="rt-angle-down"></i></h2>
            {%- endif -%}
                                <nav>
                                    <ul>
                                  {%- assign user_menu = section.settings.user_menu_handle -%}
                                  {%- for link in linklists[user_menu].links -%}
                                    <li><a href="{{ link.url }}">{{ link.title }} <i class="rt rt-arrow-right-long"></i></a></li>
                                  {%- endfor -%}
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-10 col-md-8 col-sm-12 gutter-2">
                        <div class="swiper bannerSlide2">
                            <div class="swiper-wrapper">
                                    {%- for block in section.blocks -%}
          {%- case block.type -%}
    {%- when 'slide' -%}  
                                <div class="swiper-slide"  id="block-{{ block.id }}">
                                    <div class="banner-single bg-image bg-image-3-1 bg-image-8-1">
                                        <div class="container">
                                            <div class="single-inner">
                                                <div class="content-box">
            {%- if block.settings.slide_title != '' -%}
                                                    <h2 class="slider-title">{{ block.settings.slide_title }} </h2>
            {%- endif -%}
            {%- if block.settings.slide_button_title != '' -%}
                                                    <a href="{{ block.settings.slide_button_url }}" class="slider-btn2">{{ block.settings.slide_button_title }}</a>
            {%- endif -%}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
          {%- endcase -%}
          {%- endfor -%}
                              
                            </div>
                            <div class="slider-navigation">
                                <div class="swiper-button-prev slider-btn prev"><i
                                        class="rt rt-arrow-left-long"></i></div>
                                <div class="swiper-button-next slider-btn next"><i
                                        class="rt rt-arrow-right-long"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

<style>
                                    {%- for block in section.blocks -%}
          {%- case block.type -%}
    {%- when 'slide' -%}  
.banner.banner-8 .banner-inner #block-{{ block.id }} .banner-single.bg-image-8-1 {
  background-image: url("{{ block.settings.slide_image | img_url: 'master' }}");
}
          {%- endcase -%}
          {%- endfor -%}
</style>

{%- render 'section-style' -%}

