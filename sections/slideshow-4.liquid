    <div class="banner banner-2 bg-image slideshow-4"  id="section-{{ section.id }}" data-section="SlideShow_3">
            <div class="container">
                <div class="banner-inner">
                    <div class="row align-items-center">
                                    {%- for block in section.blocks -%}
          {%- case block.type -%}
          {%- when 'slide_content' -%}
                        <div class="col-xl-4 col-lg-4 col-md-12 col-sm-12">
                            <div class="rts-heading">
                                <div class="react-image">
                                    <img src="{{ block.settings.slider_dot_image | img_url: 'large' }}" alt="{{ block.settings.slider_dot_image.alt }}">
                                </div>
                                <div class="title-inner">
                                    <div class="sub-content">
                                        <img class="line-1-img" src="{{ block.settings.title_image_1 | img_url: 'large' }}" alt="{{ block.settings.title_image_1.alt }}">
            {%- if block.settings.slide_sub_title != '' -%}
                                        <span class="sub-text">{{ block.settings.slide_sub_title }}</span>
            {%- endif -%}
                                    </div>
            {%- if block.settings.slide_title != '' -%}
                                    <h2 class="title">
                                        <span class="watermark">
                                           {{ block.settings.slide_title }}
                                        </span>
                                    </h2>
            {%- endif -%}
                                </div>
            {%- if block.settings.slide_content != '' -%}
                                <p class="description">{{ block.settings.slide_content }}
                                </p>
            {%- endif -%}
            {%- if block.settings.slide_button_title != '' -%}
                                <a href="{{ block.settings.slide_button_url }}" class="section-btn2">{{ block.settings.slide_button_title }}</a>
            {%- endif -%}
                            </div>
                        </div>
          {%- endcase -%}
          {%- endfor -%}
          
                        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-12">
                            <div class="product-item2 element-item2">
                                <div class="product-image2 image-slider-variations">
                                    <div class="swiper productSlide2">
                                        <div class="swiper-wrapper">
                                    {%- for block in section.blocks -%}
          {%- case block.type -%}
                                          {%- when 'slide_image' -%}
                                            <div class="swiper-slide">
                                                <div class="image-vari1 image-vari"><img src="{{ block.settings.slide_image | img_url: 'master' }}" alt="{{ block.settings.slide_image.alt }}">
                                                </div>
                                            </div>
          {%- endcase -%}
          {%- endfor -%}
                                          
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                      
                                    {%- for block in section.blocks -%}
          {%- case block.type -%}
          {%- when 'slide_product' -%}
                        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-12">
                            <div class="row">
                                <div class="col-xl-10 col-lg-12 col-12 side-product">
                <div class="swiper productSlide">
                    <div class="swiper-wrapper">
                                  
        {%- assign featured_product = collections[block.settings.featured_product_handle] -%}
        {%- assign featured_product_limit = block.settings.featured_product_limit_handle -%}
        {%- if featured_product == empty -%}
                        {%- assign a = 1 -%}
          {%- for i in (1..featured_product_limit) -%}
          {%- assign product_item = "product-" | append: a  -%}
          {%- assign a = a | plus: 1 -%}
                        <div class="swiper-slide">
                                    <div class="product-item element-item2">
                        {{ product_item | placeholder_svg_tag: 'product-svg-image' }}
                                        <div class="bottom-content">
                                            <a href="#" class="product-name">Maidenform Bra</a>
                                            <div class="action-wrap">
                                                <span class="price">$31.00</span>
                                            </div>
                                        </div>
                                        <div class="quick-action-button">
                                            <div class="cta-single cta-plus">
                                                <a href="#"><i class="rt-plus"></i></a>
                                            </div>
                                            <div class="cta-single cta-quickview">
                                                <button class="product-details-popup-btn"><i class="fa fa-eye"></i></button>
                                            </div>
                                            <div class="cta-single cta-wishlist">
                                                <a href="#"><i class="fa fa-heart"></i></a>
                                            </div>
                                            <div class="cta-single cta-addtocart">
                                                <a href="#"><i class="rt-basket-shopping"></i></a>
                                            </div>
                                        </div>
                                    </div>
                        </div>
          {%- if a == 7 -%}
          {%- assign a = 1 -%}
          {%- endif -%}
          {%- endfor -%}
        {%- else -%}
                      {%- for product in featured_product.products limit: featured_product_limit -%}
                        <div class="swiper-slide">
                      {%- render 'product-grid-item-6', product: product, collection: featured_product -%}
                        </div>
          {%- endfor -%}
        {%- endif -%}

                                </div>
                            </div>
                                  
                                            <div class="slider-buttons">
                                                <div class="button-prev slider-btn"><i class="rt-arrow-left-long"></i>
                                                </div>
                                                <div class="button-next slider-btn"><i class="rt-arrow-right-long"></i>
                                                </div>
                                            </div>
                                  
                                </div>
                            </div>
                        </div>

                      
          {%- endcase -%}
          {%- endfor -%}
                    </div>
                </div>
            </div>
        </div>


